{
  "name": "bootstrapedreactapp",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0",
    "build": "NODE_ENV=production webpack -p",
    "test": "jest",
    "lint": "yarn run lint-sass && yarn run lint-js",
    "lint-sass": "sass-lint -v -q",
    "lint-js": "standard --fix --verbose",
    "precommit": "yarn lint",
    "i18n": "po2json locales/fr.po assets/i18n/fr.json -f jed1.x"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "expect",
      "beforeEach",
      "fetch",
      "prompt",
      "confirm",
      "Node",
      "Event",
      "FormData",
      "MutationObserver"
    ],
    "ignore": [
      "dist/"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/app/tests/setup.js",
    "moduleNameMapper": {
      ".(scss|svg|jpg)$": "<rootDir>/app/tests/jest-empty.js",
      "components(.*)$": "<rootDir>/app/components$1",
      "containers(.*)$": "<rootDir>/app/containers$1",
      "core(.*)$": "<rootDir>/app/core$1",
      "hoc(.*)$": "<rootDir>/app/hoc$1",
      "assets(.*)$": "<rootDir>/app/assets$1",
      "locales(.*)$": "<rootDir>/app/locales$1",
      "config$": "<rootDir>/app/settings/config.js",
      "tests(.*)$": "<rootDir>/app/tests/$1",
      "app(.*)$": "<rootDir>/app/$1"
    }
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-polyfill": "^6.23.0",
    "basil.js": "^0.4.4",
    "detect-browser": "^2.0.0",
    "eventemitter3": "^2.0.2",
    "form-serialize": "^0.7.2",
    "jed": "^1.1.1",
    "js-base64": "^2.4.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.13",
    "pluralize": "^7.0.0",
    "prop-types": "^15.5.8",
    "react": "^16.4.0",
    "react-confirm": "^0.1.16",
    "react-copy-to-clipboard": "^5.0.1",
    "react-datepicker": "^0.61.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.4.0",
    "react-linkify": "^0.2.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.2.2",
    "react-tippy": "JonathanWi/react-tippy#master",
    "redux": "3.6.0",
    "redux-saga": "^0.14.3",
    "svgxuse": "^1.2.6",
    "tapestry-css": "1.9.4",
    "urijs": "^1.18.9",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.3.1",
    "cors": "^2.8.1",
    "css-loader": "^0.27.3",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "estraverse": "^4.2.0",
    "express": "^4.15.2",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.10.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.2",
    "jest": "19.0.2",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-sass": "^4.5.0",
    "po-loader": "^0.3.1",
    "po2json": "^0.4.5",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "next",
    "react-router": "^4.2.0",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.10.3",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.3",
    "sinon": "^4.0.0",
    "standard": "^9.0.1",
    "style-loader": "^0.13.2",
    "svg-sprite-loader": "^3.4.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-shell-plugin": "^0.5.0"
  },
  "author": "Guillaume Potier <cobalt2760@gmail.com>"
}
